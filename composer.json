{
    "name": "sunnysideup/push-notifications",
    "description": "Allows you to send basic push notifications to phones and desktops after users sign up",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "push",
        "progressive web-app",
        "notifications",
        "push notifications"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "minishlink/web-push": "^8.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\PushNotifications\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "bin": [
        "bin/sake-npm-build-push-notifications.sh",
        "bin/sake-npm-watch-push-notifications.sh"
    ],
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
